<nav>
    <a href="{{ url('article_index') }}" title="{{ 'articles'|trans }}">
        {{ 'articles'|trans }}
    </a>
    <a href="{{ url('category_index') }}" title="{{ 'categories'|trans }}">
        {{ 'categories'|trans }}
    </a>
    <a href="{{ url('tag_index') }}" title="{{ 'tags'|trans }}">
        {{ 'tags'|trans }}
    </a>
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        <div>{{ app.user.email }}</div>
        <div>
            <a href="{{ url('user_change_password', {'id': app.user.id}) }}">{{ 'label_user_change_password'|trans }}</a>
            <a href="{{ url('user_edit_profile', {'id': app.user.id}) }}">{{ 'label_user_edit_profile'|trans }}</a>
            <a href="{{ url('app_logout') }}">{{ 'action_logout'|trans }}</a>
        </div>
    {% else %}
        <div>
            <a href="{{ url('app_login') }}">{{ 'action_login'|trans }}</a>
            <a href="{{ url('app_register') }}">{{ 'action_register'|trans }}</a>
        </div>
    {% endif %}
</nav>
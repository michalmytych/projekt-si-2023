{% extends 'base.html.twig' %}

{% block title %}
    {{ category.name }}
{% endblock %}

{% block body %}
    <a href="{{ url('category_index') }}" title="{{ 'action.back'|trans }}">
        {{ 'action.back'|trans }}
    </a>

    <h1>{{ category.name }}</h1>

    <a href="{{ url('category_edit', {id: category.id}) }}" title="{{ 'action.edit'|trans }}">
        {{ 'action.edit'|trans }}
    </a>

{% endblock %}
